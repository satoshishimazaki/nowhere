$('.articles').html("<% @articles.each_with_index do |article, index| %><div class='col-sm-6 col-md-6 col-lg-6 nopadding'><div class='blog-box' id='article_<%= index %>''><div class='hover-bg'><%= article_image_for article.article_images.map{|articleimage| articleimage.image }.first %><div class='hover-text'><% if Time.now - article.created_at < 24*60*60 %><h5><%= 'new' %></h5><% end %><h2><%= link_to article.title, article_path(article.id) %></h2><div class='title_box'><div class='nowheretitle'><ul><li><%= article.herenowtitle %></li><li>残り時間:<% if article.left_time > 24.hours %><%= article.left_day.floor %>日<% else %><%= article.left_hour.floor %>時間<%= article.left_minute.floor %>分<% end %></li><li>View:<%= article.view_count %></li><li><span id='article_distance_<%= index %>''></span></li><% if user_signed_in?%><li><span id='article_<%= article.id %>''><%= render 'favorites/favorite_btn', article: article %></span><span id='fav_<%=article.id %>'><%= article.favorites.count %></span><% end %></li></ul></div></div></div></div></div></div><% end %>");